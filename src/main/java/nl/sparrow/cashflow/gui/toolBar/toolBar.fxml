<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="nl.sparrow.cashflow.gui.toolBar.ToolBarController">
    <children>
        <ToolBar AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                 AnchorPane.topAnchor="0.0">
            <items>
                <Button onAction="#openAccountOverview" styleClass="tool-bar-element" text="Overview"/>
                <Label text="|" styleClass="tool-bar-element"/>
                <Button onAction="#openTransactionOverview" styleClass="tool-bar-element" text="Transactions"/>
                <Label text="|" styleClass="tool-bar-element"/>
                <Button onAction="#openCsvUploadView" styleClass="tool-bar-element" text="Upload CSV"/>
            </items>
        </ToolBar>
    </children>
</AnchorPane>
